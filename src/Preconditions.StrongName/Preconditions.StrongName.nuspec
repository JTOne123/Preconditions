<?xml version="1.0"?>
<package >
  <metadata>
    <id>Preconditions.NET.StrongName</id>
    <version>1.2.1</version>
    <authors>Philippe Lécaillon</authors>
    <owners>Philippe Lécaillon</owners>
    <licenseUrl>https://github.com/lecaillon/Preconditions/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/lecaillon/Preconditions</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <summary>C# Preconditions inspired by Google Guava Preconditions. This is the signed version of the Preconditions.NET assembly.</summary>
    <description>Preconditions provide convenience static methods that help to check that a method or a constructor is invoked with proper parameter or not. In other words it checks the pre-conditions.</description>
    <releaseNotes>Add support of .NET 4.5.1.</releaseNotes>
    <copyright>Copyright © P.Lécaillon 2016</copyright>
    <tags>Preconditions Guava Check</tags>
    <dependencies>
      <group targetFramework="net451" />
      <group targetFramework=".NETStandard1.3" />
    </dependencies>
  </metadata>
  <files>
    <file src="bin\Release\net452\*.dll"            target="lib\net451" />
    <file src="bin\Release\net452\*.xml"            target="lib\net451" />

    <file src="bin\Release\netstandard1.3\*.dll"     target="lib\netstandard1.3" />
    <file src="bin\Release\netstandard1.3\*.xml"     target="lib\netstandard1.3" />
  </files>
</package>